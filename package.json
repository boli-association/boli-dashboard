{
  "name": "boli-dashboard",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts",
    "lintfix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "d3": "^7.1.1",
    "lint-staged": "^12.1.2",
    "next": "12.0.4",
    "postcss": "^8.3.11",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.4",
    "@types/node": "16.11.9",
    "@types/react": "17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-no-loops": "^0.3.0",
    "husky": "^4.3.8",
    "prettier": "2.5.0",
    "typescript": "4.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  }
}
